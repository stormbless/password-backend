{
  "name": "password-backend",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "node ./src/app.js",
    "dev": "npm run stop && concurrently \"npm run server\" \"npm run expose\"",
    "server": "nodemon ./src/app.js",
    "expose": "mapps tunnel:create -p 8302",
    "stop": "kill-port 8302",
    "test": "jest"
  },
  "nodemonConfig": {
    "watch": [
      ".env",
      "src/"
    ]
  },
  "dependencies": {
    "@mondaycom/apps-sdk": "^3.2.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-port-killer": "^1.2.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "monday-sdk-js": "^0.5.4",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@mondaycom/apps-cli": "^4.0.0",
    "concurrently": "^8.2.2",
    "debug": "~2.6.9",
    "dotenv": "^16.5.0",
    "eslint": "^v6.8.0",
    "jest": "^30.0.0-beta.3",
    "nodemon": "^3.0.2"
  }
}
